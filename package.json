{
  "name": "github-organization-manager-node",
  "version": "0.1.1",
  "description": "github manager",
  "engines": {
    "node": "12.16.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "console": "node console.js",
    "cover": "NODE_ENV=testing jest --coverage --runInBand --forceExit",
    "test": "NODE_ENV=testing jest --runInBand --forceExit",
    "test-inspect": "NODE_ENV=testing node --inspect --debug-brk jest",
    "eslint-check": "eslint --print-config .eslintrc.js --ignore-pattern ./.eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint \"**/*.js\" --ignore-pattern ./.eslintrc.js",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint-fix": "eslint \"**/*.js\" --fix --ignore-pattern ./.eslintrc.js",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "start": "node server.js",
    "start-dev": "nodemon --inspect server.js"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "author": "Wolox",
  "homepage": "git@github.com:gonziesc/github-organization-manager-node.git",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:gonziesc/github-organization-manager-node.git.git"
  },
  "bugs": {
    "url": "git@github.com:gonziesc/github-organization-manager-node.git/issues",
    "email": "tls@wolox.com.ar"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "**/*.js",
      "!**/console.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/migrations/**",
      "!**/config/**",
      "!**/scripts/**"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@octokit/auth-app": "^2.4.2",
    "@octokit/rest": "^16.43.1",
    "@sendgrid/mail": "^6.5.2",
    "auth0": "^2.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "express-wolox-logger": "1.1.0",
    "jwks-rsa": "^1.6.2",
    "jwt-simple": "^0.5.1",
    "umzug": "^2.1.0"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-preset-es2015": "6.24.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-wolox": "^4.0.0",
    "eslint-config-wolox-node": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "istanbul": "^0.4.3",
    "jest": "^25.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prompt": "^1.0.0",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
